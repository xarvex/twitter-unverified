{
    "manifest_version": 2,
    "name": "Twitter Unverified",
    "version": "0.0.0",
    "description": "Hide Tweets and replies from verified Twitter users.",
    "content_scripts": [
        {
            "matches": [
                "*://*.twitter.com/*/status/*"
            ],
            "js": [
                "reply.js"
            ],
            "css": [
                "reply.css"
            ]
        }
    ]
}
