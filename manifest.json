{
    "manifest_version": 2,
    "name": "Twitter Unverified",
    "version": "0.0.0",
    "description": "Hide Tweets and replies from Twitter Blue users.",
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "*://*.twitter.com/*"
            ],
            "js": [
                "inject.js"
            ]
        },
        {
            "matches": [
                "*://*.twitter.com/*"
            ],
            "js": [
                "annoyance.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "xhr.js"
    ]
}
